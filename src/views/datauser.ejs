<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="/src/public/bootstrap.min.css">

    <title>Hello, world!</title>
  </head>
  <body>
    <h1>INI HALAMAN DATA USER </h1>
    <% rows.forEach(user => { %>
        <ul>
            <li>ID User       : <%= user.id_user %> </li>
            <li>Kode Karyawan : <%= user.kode_karyawan %> </li>
            <li>Nama Karyawan : <%= user.nama_karyawan %> </li>
            <li>Jabatan       : <%= user.jabatan %> </li>
        </ul>

   <% }) %>

    <script src="/src/public/jquery-3.3.1.slim.min.js"></script>
    <script src="/src/public/popper.min.js"></script>
    <script src="/src/public/bootstrap.min.js"></script>
  </body>
</html>